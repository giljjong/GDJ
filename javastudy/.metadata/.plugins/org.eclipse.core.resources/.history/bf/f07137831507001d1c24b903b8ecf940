package ex03_number;

public class Ex3 {
	public static void main(String[] args) {
		
		// 대입 연산
		int score = 100; // 등호(=)가 대입 연산자이다.
		System.out.println(score);
		
		// 연습
		// x 에 10이 있고, y에 20 이 있다.
		// x 와 y 에 저장된 값을 서로 교환하시오.
		int x = 10;
		int y = 20;
		int temp;
		
		temp = x;
		x = y;
		y = temp;
		
		System.out.println(x);
		System.out.println(y);
		
	}
}
